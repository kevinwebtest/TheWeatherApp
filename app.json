{
  "expo": {
    "name": "The Weather App",
    "slug": "TheWeatherApp",
    "description": "Weather app lets you know the weather of your current location.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-weather.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-weather.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
